{
  "expo": {
    "name": "Atives - Creative Community",
    "slug": "atives",
    "version": "1.0.0",
    "description": "A platform for creatives to showcase work, find opportunities, and connect with fellow creators",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "atives",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.ashftw.ativesdemo"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "93a5427c-6ad1-44b0-add0-45b1d29ed073"
      }
    }
  }
}
